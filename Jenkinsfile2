pipeline {
    agent any
    stages {
		stage ('Initialize') {
			steps {
				echo "Initializing the code file"
			}
		}

		stage ('Build') {
			steps {
				echo "Build - successful"
				echo "Hello world"
			}
		}

		stage ('Deploy') {
			steps {
				echo "Stage Deploy - successful"
			}
		}
		

	}
}